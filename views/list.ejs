<%- include('header') -%>
  <div class="box" id="heading">
    <h1><%= listItem %></h1>
  </div>

  <div class="box">
    <% for(i=0;i<Items.length;i++){%>
    <div class="item">
      <input type="checkbox">
      <p><%= Items[i] %></p>
    </div>
    <%}%>

      <form class="item" action="/" method="post">
        <input type="text" name="NewItem" placeholder="New Item" autocomplete="off" autofocus>
        <button type="submit" name="list" value=<%= listItem %>>+</button>
    </form>
  </div>

<%- include('footer') -%>
